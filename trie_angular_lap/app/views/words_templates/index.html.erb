<h2>YOU SHOULD NEVER SEE ME!</h2>
{{words.greeting}}
{{words.Word}}
<div ng-repeat="word in words.wordsArr">
	<span>Name: {{word.name}} </span>
	<span>Description: {{word.description}}</span>
</div>

<form ng-submit="words.addWord()">
	<input type="text" ng-change="words.autoComplete(words.newWord.name)" ng-model="words.newWord.name">
	<input type="text" ng-model="words.newWord.description">
	<button>Submit</button>
</form>

<div class="auto-comp">
	<div ng-repeat="wordList in words.autoComp">{{wordList}}</div>
</div>